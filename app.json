{
  "expo": {
    "name": "Harvence",
    "slug": "harvence",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "harvence",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3355ff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pybash.harvence"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#3355ff"
      },
      "package": "com.pybash.harvence",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Plain-Regular.ttf",
            "./assets/fonts/Plain-Medium.ttf",
            "./assets/fonts/Plain-Bold.ttf"
          ]
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7c3f82cc-2ac9-4488-8765-26458400ae6c"
      }
    },
    "owner": "pybash"
  }
}
